service FUNCP_MEMORY
{
    server hw (bsv, connection) <- sw (cpp, method)
    {
        method Invalidate(in MEM_ADDRESS[32] addr);
    };

    server sw (cpp, method) <- hw (bsv, connection)
    {
        method Load (in MEM_ADDRESS[32]    addr, out MEM_VALUE[32] data);
        method Store(in MEM_STORE_INFO[64] info);
    };
};
